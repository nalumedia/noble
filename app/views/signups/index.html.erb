<h1>Listing signups</h1>

<table>
  <thead>
    <tr>
      <th>Signup name</th>
      <th>Signup email</th>
      <th>Signup phone</th>
      <th>Signup status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @signups.each do |signup| %>
      <tr>
        <td><%= signup.signup_name %></td>
        <td><%= signup.signup_email %></td>
        <td><%= signup.signup_phone %></td>
        <td><%= signup.signup_status %></td>
        <td><%= link_to 'Show', signup %></td>
        <td><%= link_to 'Edit', edit_signup_path(signup) %></td>
        <td><%= link_to 'Destroy', signup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Signup', new_signup_path %>
